{% schema %}
{
  "name": "FAQ's Question-Answers",
  "settings": [
    {
      "type": "header",
      "content": "Section Heading"
    },
    {
      "type": "checkbox",
      "id": "rjee_faqs_section_show_hide",
      "label": "Show FAQ Section",
      "default": true,
      "info": "Enable/Disable FAQs section."
    },
    {
      "type": "text",
      "id": "rjee_faqs_title",
      "label": "Header Text",
      "default": "FAQ > ORDER/PAYMENT/CANCELLATION",
      "info": "Header text to display."
    },
    {
      "type": "textarea",
      "id": "rjee_faqs_sub_title",
      "label": "Section Sub Title",
      "default": "Section Sub Title Here"
    }
  ],
  "blocks":[
    {
      "type":"rjee_faq",
      "name": "Question-Answers",
      "settings": [
        {
          "type": "text",
          "id": "rjee_faqs_question_text",
          "label": "Title",
          "default": "FAQ Question Heading text to display."
        },
        {
          "type": "richtext",
          "id": "rjee_faqs_answer_text",
          "label": "Content",
          "default": "<p>FAQ Answer text to display</p>"
        }
      ]
    },
    {
      "type": "rjee_section_margin",
      "name": "Section Margin",
      "limit": 1,
      "settings": [
        {
          "type": "header",
          "content": "Large/Desktop Device"
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_margin_top",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Margin Top",
          "unit": "px",
          "default": 0
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_margin_bottom",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Margin Bottom",
          "unit": "px",
          "default": 0
        },
        {
          "type": "header",
          "content": "Tablet Device"
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_margin_top_md",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Margin Top",
          "unit": "px",
          "default": 0
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_margin_bottom_md",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Margin Bottom",
          "unit": "px",
          "default": 0
        },
        {
          "type": "header",
          "content": "Mobile Device"
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_margin_top_xs",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Margin Top",
          "unit": "px",
          "default": 0
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_margin_bottom_xs",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Margin Bottom",
          "unit": "px",
          "default": 0
        }
      ]
    },
    {
      "type": "rjee_section_padding",
      "name": "Section Padding",
      "limit": 1,
      "settings": [
        {
          "type": "header",
          "content": "Large/Desktop Device"
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_padding_top",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Padding Top",
          "unit": "px",
          "default": 40
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_padding_bottom",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Padding Bottom",
          "unit": "px",
          "default": 100
        },
        {
          "type": "header",
          "content": "Tablet Device"
        },
        {
          "type": "range",
          "id": "rjee_faqs_section_padding_top_md",
          "min": 0,
          "max": 150,
          "step": 5,
          "label": "Padding Top",
          "unit": "px",
          "default": 30
        },
