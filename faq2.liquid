        {%- if block.settings.section_bg_image -%}
          data-bgset="{%- render 'responsive-bg-image', image: block.settings.section_bg_image -%}"
        {%- endif -%}
    {%- endcase -%}
  {%- endfor -%}
{%- endcapture -%}

